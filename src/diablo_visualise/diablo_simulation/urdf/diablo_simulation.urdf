  <!-- ==================== LIDAR传感器 ==================== -->
  <link name="lidar_link">
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.05"/>
      </geometry>
      <material name="blue">
        <color rgba="0 0 0.8 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.05"/>
      </geometry>
    </collision>
  </link>

  <!-- ==================== 关节定义 ==================== -->
  <joint name="lidar_joint" type="fixed">
    <parent link="base_link"/>
    <child link="lidar_link"/>
    <origin xyz="0 0 0.45" rpy="0 0 0"/>
  </joint>

  <joint name="Rev1" type="continuous">
    <origin rpy="0 0 0" xyz="0.00036 -0.053347 0.379"/>
    <parent link="base_link"/>
    <child link="motor_right_link_1"/>
    <axis xyz="0.0 -1.0 0.0"/>
  </joint>
  <joint name="Rev2" type="continuous">
    <origin rpy="0 0 0" xyz="0.00036 0.053553 0.379"/>
    <parent link="base_link"/>
    <child link="motor_left_link_1"/>
    <axis xyz="0.0 1.0 0.0"/>
  </joint>
  <joint name="Rev3" type="continuous">
    <origin rpy="0 0 0" xyz="0.0 0.1181 0.0"/>
    <parent link="motor_left_link_1"/>
    <child link="leg_left_link_1"/>
    <axis xyz="0.0 1.0 0.0"/>
  </joint>
  <joint name="Rev4" type="continuous">
    <origin rpy="0 0 0" xyz="0.0 0.016 -0.14"/>
    <parent link="leg_left_link_1"/>
    <child link="leg2_left_link_1"/>
    <axis xyz="0.0 1.0 -0.0"/>
  </joint>
  <joint name="Rev5" type="continuous">
    <origin rpy="0 0 0" xyz="0.0 0.0277 -0.14"/>
    <parent link="leg2_left_link_1"/>
    <child link="wheel_right_link_1"/>
    <axis xyz="0.0 1.0 0.0"/>
  </joint>
  <joint name="Rev6" type="continuous">
    <origin rpy="0 0 0" xyz="0.0 -0.1181 0.0"/>
    <parent link="motor_right_link_1"/>
    <child link="leg_right_link_1"/>
    <axis xyz="0.0 -1.0 0.0"/>
  </joint>
  <joint name="Rev7" type="continuous">
    <origin rpy="0 0 0" xyz="0.0 -0.016 -0.14"/>
    <parent link="leg_right_link_1"/>
    <child link="leg2_right_link_1"/>
    <axis xyz="0.0 -1.0 -0.0"/>
  </joint>
  <joint name="Rev8" type="continuous">
    <origin rpy="0 0 0" xyz="0.0 -0.0277 -0.14"/>
    <parent link="leg2_right_link_1"/>
    <child link="wheel_left_link_1"/>
    <axis xyz="0.0 -1.0 -0.0"/>
  </joint>

  <!-- ==================== 传感器插件 ==================== -->
  <gazebo reference="lidar_link">
    <sensor name="lidar" type="ray">
      <pose>0 0 0.5 0 0 0</pose>
      <visualize>true</visualize>
      <update_rate>10</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>720</samples>
            <resolution>1</resolution>
            <min_angle>-3.1415</min_angle>
            <max_angle>3.1415</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.12</min>
          <max>12.0</max>
          <resolution>0.01</resolution>
        </range>
        <noise>
          <type>gaussian</type>
          <mean>0.0</mean>
          <stddev>0.01</stddev>
        </noise>
      </ray>
      <plugin name="lidar_plugin" filename="libgazebo_ros_ray_sensor.so">
        <topic_name>/scan</topic_name>
        <frame_name>lidar_link</frame_name>
      </plugin>
    </sensor>
  </gazebo>

  <!-- ==================== 控制系统 ==================== -->
  <gazebo>
    <plugin name="diff_drive" filename="libgazebo_ros_diff_drive.so">
      <ros>
        <namespace>/</namespace>
      </ros>
      <update_rate>50</update_rate>
      <left_joint>Rev1</left_joint>
      <right_joint>Rev2</right_joint>
      <wheel_separation>1.2</wheel_separation>
      <wheel_diameter>0.3</wheel_diameter>
      <max_wheel_torque>20</max_wheel_torque>
      <command_topic>cmd_vel</command_topic>
      <odometry_topic>odom</odometry_topic>
      <odometry_frame>odom</odometry_frame>
      <robot_base_frame>base_link</robot_base_frame>
    </plugin>

    <plugin name="ros_control" filename="libgazebo_ros_control.so">
      <robotNamespace>/diablo</robotNamespace>
      <controlPeriod>0.001</controlPeriod>
    </plugin>
  </gazebo>
</robot>
